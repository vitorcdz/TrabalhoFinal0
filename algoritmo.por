programa {
  funcao inicio() {
    inteiro n1,n2
      escreva("Digite um número : ")
      leia(n1)
      escreva("Digite um número para ser somado: ")
      leia(n2)
      escreva("A soma desses números resulta em ",n1+n2)
      escreva("\nDigite um número : ")
      leia(n1)
      escreva("Digite um número para multiplicar: ")
      leia(n2)
      escreva("A multiplicação desses números resulta em: ",n1*n2)
  }
}

